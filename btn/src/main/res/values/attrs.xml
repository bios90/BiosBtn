<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BiosBtn">
        <attr name="gradient" format="boolean" />
        <attr name="background_color" format="reference" />
        <attr name="color_1" format="reference" />
        <attr name="color_2" format="reference" />
        <attr name="corner_radius" format="dimension" />
        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_color" format="reference" />
        <attr name="text_color" format="reference" />
        <attr name="btn_font" format="string" />
        <attr name="android:textSize" format="dimension" />
        <attr name="android:padding" format="dimension" />
        <attr name="text" format="string" />
        <attr name="text_padding_horizontal" format="dimension" />
        <attr name="faw_text" format="string" />
        <attr name="icon_padding_horizontal" format="dimension" />
        <attr name="icon_padding_vertical" format="dimension" />
        <attr name="icon_size_faw" format="dimension" />
        <attr name="icon_color" format="reference" />
        <attr name="min_width" format="dimension"/>
        <attr name="max_width" format="dimension"/>

        <attr name="gradient_direction">
            <enum name="bottom_to_top" value="0" />
            <enum name="left_to_right" value="1" />
        </attr>

        <attr name="btn_type">
            <enum name="no_icon" value="0" />
            <enum name="icon_left_end" value="1" />
            <enum name="icon_right_end" value="2" />
            <enum name="icon_left_center" value="3" />
            <enum name="icon_right_center" value="4" />
        </attr>

    </declare-styleable>
</resources>